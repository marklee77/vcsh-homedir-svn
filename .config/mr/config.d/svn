[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/svn.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-svn.git svn
           vcsh svn remote set-url --push origin git@stillwell.me:mark/vcsh-homedir-svn.git
           vcsh svn remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-svn.git
           vcsh svn remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-svn.git
